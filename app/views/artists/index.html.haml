%h1 Artists

= link_to 'New Artist', new_artist_path

%table
  %thead
    %tr
      %th Artist Names
  %tbody
    - @artists.each do |artist|
      %tr
        %td #{link_to artist.name, artist_path(artist.id)} 
        %td= link_to('edit', edit_artist_path(artist.id), {method: :get})
        %td= link_to('delete', artist_path(artist.id), {method: :delete, confirm: 'Are you sure you want to delete this artist?'}) 